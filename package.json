{
  "name": "mole-api",
  "version": "0.0.1",
  "description": "a home for the moles",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && npm run build-babel",
    "build-babel": "./node_modules/.bin/babel ./src -d ./build -s",
    "start": "nodemon --watch src --exec 'npm run local'",
    "local": "npm run build && NODE_PATH=src/ node -r @babel/register -r dotenv/config build dotenv_config_path=.env.local"
  },
  "dependencies": {
    "apollo-server": "^2.13.1",
    "apollo-server-express": "^2.13.1",
    "express": "^4.17.1",
    "firebase-admin": "^8.12.1",
    "graphql": "^15.0.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-resolvers": "^0.4.2",
    "graphql-subscriptions": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "graphql-tools": "^6.0.0"
  }
}
