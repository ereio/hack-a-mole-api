{
  "name": "mole-api",
  "version": "0.0.1",
  "description": "a home for the moles",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch src --exec 'yarn local'",
    "local": "yarn build && NODE_PATH=src/ node -r @babel/register -r dotenv/config build dotenv_config_path=.env",
    "build": "yarn clean && yarn build-babel",
    "build-babel": "./node_modules/.bin/babel ./src -d ./build -s",
    "clean": "rm -rf build && mkdir build",
    "init-eslint": "eslint --init"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "apollo-server": "^2.13.1",
    "apollo-server-express": "^2.13.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.12.1",
    "graphql": "^15.0.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-resolvers": "^0.4.2",
    "graphql-subscriptions": "^1.1.0",
    "helmet": "^3.22.0"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "graphql-tools": "^6.0.0",
    "nodemon": "^2.0.4"
  }
}
